resource_registry:
  OS::TripleO::Services::IpaClient: /usr/share/openstack-tripleo-heat-templates/deployment/ipa/ipaservices-baremetal-ansible.yaml

parameter_defaults:
  DnsSearchDomains: ["{{ domain_name }}"]
  CloudDomain: {{ domain_name }}
  CloudName: overcloud.{{ domain_name }}
  CloudNameInternal: overcloud.internalapi.{{ domain_name }}
  CloudNameStorage: overcloud.storage.{{ domain_name }}
  CloudNameStorageManagement: overcloud.storagemgmt.{{ domain_name }}
  CloudNameCtlplane: overcloud.ctlplane.{{ domain_name }}
  IdMServer: idm.{{ domain_name }}
  IdMDomain: {{ domain_name }}
  IdMInstallClientPackages: False
  EnableEtcdInternalTLS: True
  MemcachedTLS: true
  MemcachedPort: 11212