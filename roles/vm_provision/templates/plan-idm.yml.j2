parameters:
  rhnuser: {{ rhnuser | quote }}
  rhnpassword: {{ rhnpassword | quote }}
  skip_iso: true

idm:
  image: "rhel8"
  memory: 8192
  numcpus: 8
  disks:
   - size: {{ disk_size_root }}
     interface: "sata"
  nets: 
{% for net in ['default'] + (libvirt_networks | map(attribute='name') | list) %}
    - {{ net }}
{% endfor %}